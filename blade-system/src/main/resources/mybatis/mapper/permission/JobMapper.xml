<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blade.manager.system.modules.permission.mapper.JobMapper">
    <resultMap id="jobMap" type="com.blade.manager.system.modules.permission.entity.Job">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="enabled" jdbcType="TINYINT" property="enabled"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="sort" jdbcType="BIGINT" property="sort"/>
        <result column="dept_id" jdbcType="BIGINT" property="deptId"/>
    </resultMap>

    <sql id="allColumn">
        id, name, enabled, create_time, sort, dept_id
    </sql>
    
    <select id="selectPage" resultMap="jobMap">
        SELECT <include refid="allColumn" /> FROM job
    </select>
</mapper>
